
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="./css/TableUser.css">
    <title>Admin - Danh sách người dùng</title>
</head>
<body>
    <div class="container">
        <h2>Danh sách người dùng</h2>
        <table class="user-table">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Tên</th>
                    <th>Email</th>
                    <th>Quyền</th>
                </tr>
            </thead>
            <tbody>
                <!-- Dữ liệu người dùng sẽ được đặt ở đây -->
                <tr>
                    <td>1</td>
                    <td>Người dùng 1</td>
                    <td>user1@example.com</td>
                    <td>Admin</td>
                </tr>
                <tr>
                    <td>2</td>
                    <td>Người dùng 2</td>
                    <td>user2@example.com</td>
                    <td>Thành viên</td>
                </tr>
                <!-- Thêm các dòng khác tương tự cho các người dùng khác -->
            </tbody>
        </table>
    </div>
</body>
</html>
